@using ScottybonsMVC.Models.ViewModels
@using ScottybonsMVC.Models.ViewModels.Customer
@inherits UmbracoTemplatePage
@{
    Layout = "ScottyBonsMaster.cshtml";
    var greenBar = "Login";
}

@section BodyContent {
    @if (@Request["Nav"] == "IsForOrder")
    {
       
    <section id ="top-menu">
        @Html.Partial("Account/_GreenBar", greenBar)
    </section>


    }
    else
    {
        <section id ="top-menu">
            <div id="register-top_no_menu">
            </div>
        </section>
    }
    @Html.Partial("Account/_Login", new LoginVm())
 
       }
